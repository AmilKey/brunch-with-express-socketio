    "ugly-blanket-brunch": "~1.5.3",
    "async": "*",
    "socket.io": "*",
    "express": "*",
    "jade": "~0.28.2",
    "debug": "~0.7.2",
    "ansi-color": "*",
    "date-utils": "*",
